# Sheikh Project Environment Variables

# Backend Configuration
BACKEND_PORT=8000
SANDBOX_URL=http://localhost:8080

# Cloudflare AI Gateway Configuration
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_GATEWAY_ID=your_gateway_id
CLOUDFLARE_API_KEY=your_api_key

# Cloudflare AI Gateway Default Headers
CLOUDFLARE_DEFAULT_CACHE_TTL=3600
CLOUDFLARE_DEFAULT_MAX_ATTEMPTS=3
CLOUDFLARE_DEFAULT_RETRY_DELAY=1000
CLOUDFLARE_DEFAULT_REQUEST_TIMEOUT=30000
CLOUDFLARE_DEFAULT_BACKOFF_TYPE=exponential
CLOUDFLARE_DEFAULT_COLLECT_LOG=true

# AI Provider API Keys
OPENAI_API_KEY=your_openai_api_key
GOOGLE_AI_API_KEY=your_google_ai_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key
FAL_AI_API_KEY=your_fal_ai_api_key
CARTESIA_API_KEY=your_cartesia_api_key

# Frontend Configuration
REACT_APP_BACKEND_URL=http://localhost:8000
REACT_APP_CLOUDFLARE_TOKEN=your_cloudflare_token
REACT_APP_OPENAI_API_KEY=your_openai_api_key